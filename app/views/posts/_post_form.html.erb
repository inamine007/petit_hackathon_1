<h1>記事の作成</h1>
<%= form_with(model: @post, local: true) do |f| %>
    <div class="form-group mt-3">
        <%= f.label :タイトル %>
        <%= f.text_field :title, class: "form-control" %>
    </div>
    <div class="form-group mt-3">
        <%= f.label :写真 %>
        <%= f.file_field :image%>
    </div>
    <div class="mt-3">
        <%= f.submit "投稿", class: "btn btn-primary" %>
    </div>
<% end %>