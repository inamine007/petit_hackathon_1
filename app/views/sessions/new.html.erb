<h1>ログイン</h1>
<div>
    <%= form_with(url: login_path, scope: :session, local: true) do |f| %>
    <%# <%= render 'users/error_messages' %> 
    <ul>
        <li>
            <%= f.label :メールアドレス %>
            <%= f.email_field :email %>
        </li>
        <li>
            <%= f.label :パスワード %>
            <%= f.password_field :password %>
        </li>
    </ul>
    <div><%= f.submit "ログイン" %></div>
    <% end %>
</div>
